    "build-service-registry:arbitrum": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-arbitrum/subgraph.arbitrum.yaml",
    "build-service-registry:base": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-base/subgraph.base.yaml",
    "build-service-registry:celo": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-celo/subgraph.celo.yaml",
    "build-service-registry:gnosis": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-gnosis/subgraph.gnosis.yaml",
    "build-service-registry:mode": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-mode/subgraph.mode.yaml",
    "build-service-registry:optimism": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-optimism/subgraph.optimism.yaml",
    "build-service-registry:polygon": "yarn codegen-service-registry-l2 && graph build subgraphs/service-registry/service-registry-polygon/subgraph.polygon.yaml",
    "build-service-registry:debug": "yarn codegen-service-registry-debug && graph build subgraphs/service-registry/service-registry-debug/subgraph.yaml",
    "build-staking:mainnet": "yarn codegen-staking && graph build subgraphs/staking/subgraph.mainnet.yaml",
    "build-staking:arbitrum": "yarn codegen-staking && graph build subgraphs/staking/subgraph.arbitrum-one.yaml",
    "build-staking:base": "yarn codegen-staking && graph build subgraphs/staking/subgraph.base.yaml",
    "build-staking:celo": "yarn codegen-staking && graph build subgraphs/staking/subgraph.celo.yaml",
    "build-staking:gnosis": "yarn codegen-staking && graph build subgraphs/staking/subgraph.gnosis.yaml",
    "build-staking:polygon": "yarn codegen-staking && graph build subgraphs/staking/subgraph.matic.yaml",
    "build-staking:mode": "yarn codegen-staking && graph build subgraphs/staking/subgraph.mode-mainnet.yaml",
